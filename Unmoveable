import javax.swing.*;
import java.awt.*;
import java.awt.event.*;
import java.io.*;
import java.awt.geom.*;
import java.awt.image.*;


public class Unmoveable extends Skin {

	
	public Unmoveable() {
		// TODO Auto-generated constructor stub
	}
	public Unmoveable (int i){
		Toolkit T = Toolkit.getDefaultToolkit();
		switch(i){
		case 0:
			test="grass";
			image = T.getImage("grass_background.jpg");
			break;
		case 1:
			test="wall*";
			image = T.getImage("wall_gray.jpg");
			break;
		case 2:
			test="path*";
			image = T.getImage("wall_light_gray.jpg");
			break;
		}
	}

}
